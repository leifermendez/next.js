{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "next/dist/compiled/*": ["./dist/compiled/*"],
      "next/dist/esm/*": ["./src/*"],
      "next/dist/*": ["./src/*"]
    }
  },
  "include": [
    "./*.d.ts",
    "future/*.d.ts",
    "image-types/global.d.ts",
    "compat/*.d.ts",
    "legacy/*.d.ts",
    "navigation-types/*.d.ts",
    "navigation-types/compat/*.d.ts",
    "experimental/**/*.d.ts",
    "font",
    "types/index.d.ts",
    "types/global.d.ts",
    "types/compiled.d.ts",
    "types/compiled-shared.d.ts"
  ],
  "exclude": ["dist"]
}
